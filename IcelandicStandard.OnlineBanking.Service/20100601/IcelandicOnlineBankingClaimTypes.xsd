          </xs:element>
          <xs:element name="DiscountAmount" type="ct:Amount" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en-US">The discount amount that is due to be granted.</xs:documentation>
              <xs:documentation xml:lang="is-IS">Afsláttur sem verður veittur þegar innheimtukrafa er greidd miðað við stöðu.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrincipalRemaining" type="ct:Amount" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en-US">TODO: Ensk</xs:documentation>
              <xs:documentation xml:lang="is-IS">Heildar upphæð sem greidd hefur verið inn á kröfu.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SecondaryCollectionIdentifier" type="clt:Identifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en-US">TODO english translation. </xs:documentation>
              <xs:documentation xml:lang="is-IS">Auðkenni milliinnheimtuaðila sem krafan er tengd við milliinnheimtumerkingu.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LegalCollectionIdentifier" type="clt:Identifier" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="en-US">TODO english translation.</xs:documentation>
              <xs:documentation xml:lang="is-IS">Auðkenni löginnheimtuaðila sem krafan er tengd við lögfræðimerkingu.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CurrencyExchangeRate">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Information about currency.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Samsett svæði fyrir gengisupplýsingar.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Currency" type="ct:CurrencyCode">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The currency of the claim.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Mynt innheimtukröfunnar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Rate" type="ct:CurrencyAmount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The exchange rate used.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Upplýsingar um gengi gjaldmiðils innheimtukröfu.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Payment">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Describes a discrete payment.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Lýsir stakri greiðslu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Key" type="clt:ClaimKey">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The combination of fields that uniquely identifies a claim.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Samsett svæði fyrir einkvæman lykil innheimtukröfu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayorID" type="ct:PersonID">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Payor person id.</xs:documentation>
          <xs:documentation xml:lang="is-IS"> Kennitala kröfugreiðanda.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Amount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Amount of claim.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Upphæð innheimtukröfu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="FinalDueDate" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Final due date of claim.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Eindagi innheimtukröfu</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Identifier" type="clt:Identifier">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The unique identifier of the claimant that the claim relates to.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Auðkenni kröfuhafa sem innheimtukrafan tengist.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Reference" type="clt:Reference">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Reference determined by claimant.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Tilvísun ákveðin af kröfuhafa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CategoryCode" type="ct:CategoryCode" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Claimant’s text key, explanation of payment. Optional property of claim.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Textalykill kröfuhafa, skýring á greiðslu. Valkvætt eigindi á innheimtukröfu.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="RedeemingBank" type="ct:Bank">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Where the payment comes from.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Sá banki sem greiðsla kemur frá.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionDate" type="xs:dateTime">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The date on which a transaction in the Claims File is effected.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Sá dagur sem uppfærsla á kröfuskrá er framkvæmd á.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BookingDate" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The date when the transaction is booked.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dagsetning þegar færsla er bókuð hjá RB.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ValueDate" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="is-IS">The validation date used to calculate default interests and charges.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Fjárhagsleg viðmiðunardagssetning útreikninga á vöxtum og gjöldum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PaymentType">
        <xs:simpleType>
          <xs:annotation>
            <xs:documentation xml:lang="en-US">Types of payment, complete payment, partial payment or refund payment.</xs:documentation>
            <xs:documentation xml:lang="is-IS">Tegund greiðslu, fullnaðargreiðsla, innáborgun, endurgreiðsla.</xs:documentation>
          </xs:annotation>
          <xs:restriction base="xs:string">
            <xs:enumeration value="Complete">
              <xs:annotation>
                <xs:documentation xml:lang="en-US">The claim has been paid fully paid.</xs:documentation>
                <xs:documentation xml:lang="is-IS">Innheimtukrafan hefur verið greidd að fullu.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Partial">
              <xs:annotation>
                <xs:documentation xml:lang="en-US">The payment was partial.</xs:documentation>
                <xs:documentation xml:lang="is-IS">Greitt var að hluta.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="Refund">
              <xs:annotation>
                <xs:documentation xml:lang="en-US">The payment was a refund.</xs:documentation>
                <xs:documentation xml:lang="is-IS">Greidd endurgreiðslukrafa.</xs:documentation>
              </xs:annotation>
            </xs:enumeration>
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="AmountDeposited" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Amount deposited in the account used for the disposal of payments.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Upphæð sem lögð var inn til ráðstöfunar í reikning kröfuhafa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TotalAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Total amount paid, with default interest and costs.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Heildarupphæð greidd með áföllnum vöxtum og gjöldum.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CapitalGainsTax" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Withheld capital gains tax.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Afdreginn fjármagnstekjuskattur.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BillNumber" type="ct:BillNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Billnumber, optional. Information provided by claimant.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Seðilnúmer, valkvætt. Upplýsingar settar af kröfuhafa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CustomerNumber" type="clt:CustomerNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Customer number of payor, claimant's unique code for the customer.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Svæði fyrir einkvæmt viðskiptanúmer greiðanda úr kerfi kröfuhafa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="NoticeChargeAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Notice charge paid.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Greitt tilkynningargjald.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DefaultChargeAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Default charge paid.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Greitt vanskilagjald.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OtherCostsAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Other costs paid.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Greiddur annar kostnaður.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OtherDefaultCostsAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Other default costs paid.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Greiddur annar vanskilakostnaður.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DefaultInterestAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Paid default interest.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Greiddir dráttarvextir.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DiscountAmount" type="ct:Amount">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Discount granted.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Veittur afsláttur.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Currency" type="clt:CurrencyExchangeRate" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Currency information, only available for forex claims.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Gengisupplýsingar, aðeins á innheimtukröfum í erlendu gengi.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BatchNumber" type="ct:BatchNumber" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Batch number of payment.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Bunkanúmer greiðslu.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClaimError">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Information about claim that has generated an error.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Upplýsingar um innheimtukröfu sem lenti á villu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Key" type="clt:ClaimKey"/>
      <xs:element name="Error" type="ct:Error">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Information about an error when processing a claim.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Villa sem kom upp við aðgerð á innheimtukröfu.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClaimResult">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">The result of an operation on a claim. If is was created it contains information indicating whether it should be printed and if it is connected to an automatic debit aggreement.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Niðurstaða af aðgerð á innheimtukröfu. Ef hún var stofnuð eru upplýsingar um hvor það eigi að prenta hana og hvort hún tengist beingreiðslusamningi.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Key" type="clt:ClaimKey"/>
      <xs:element name="Print" type="clt:PrintInformation">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Indicates whether the payment slip needs to be printed or not. If the claimant has an agreement with the bank, it will handle the printing.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Segir til um hvort prenta þarf innheimtukröfu. Bankastofnun sér um útprentun ef um það er samkomulag.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClaimOperationResult">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">The results returned from operations on claims. A list of claims and/or errors is returned. Information about printing and direct payment is only relevant to creation of claims.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Upplýsingar um niðurstöðu aðgerðar. Skilað er lista af innheimtukröfum og/eða villum. Upplýsingar um prentun og beingreiðslu eiga aðeins við þegar innheimtukrafa er stofnuð.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ID" type="ct:OperationID">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The unique identifier of the operation. If an error has occured, this can return an empty Id.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Auðkenni aðgerðar á stakri innheimtukröfu. Getur verið tómt ef villa kom upp.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="clt:BatchStatus">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Status of the batch of claims .</xs:documentation>
          <xs:documentation xml:lang="is-IS">Staða mengis innheimtukrafna sem aðgerð var framkvæmd á.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Success" type="clt:ClaimResult" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">List of claims whose operation was a success.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Listi yfir innheimtukröfur sem aðgerð tókst á.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Errors" type="clt:ClaimError" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">List of claims with errors.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Listi yfir innheimtukröfur sem lentu á villu.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AlterClaims">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Alter claims that have been submitted to the system.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Breyta innheimtukröfum sem búið er að skrá í kerfið.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Claim" type="clt:Claim" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SecondaryCollectionPaymentsQuery">
