  <xs:complexType name="ClaimInfo">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Information about the current status of a claim.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Upplýsingar um stöðu innheimtukröfu.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="clt:Claim">
