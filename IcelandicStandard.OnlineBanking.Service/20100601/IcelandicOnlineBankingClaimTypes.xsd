      <xs:documentation xml:lang="en-US">Query on a secondary collection payment.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Fyrirspurn um greiðslu á milliinnheimtu</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Claimant" type="ct:PersonID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The Person ID of the claimant whose data is being queried.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Kennitala þess kröfuhafa sem verið er að fletta gögnum upp fyrir.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionDateFrom" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date from inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags frá og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionDateTo" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryFrom" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry from inclusive will be returned, starting from 1.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skilar færslum frá og með. Fyrsta færsla hefur númer 1</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryTo" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Færsla til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SecondaryCollectionClaimsQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Query on claims in secondary cellection.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Fyrirspurn um innheimtukröfur í milliinnheimtu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Claimant" type="ct:PersonID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The persion Id of the claimant.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Kennitala kröfuhafa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollectionDateFrom" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date from inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags frá og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CollectionDateTo" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="clt:ClaimStatus" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Status of the claims.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Staða innheimtukrafna.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryFrom" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry from inclusive will be returned, starting from 1.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skilar færslum frá og með. Fyrsta færsla hefur númer 1</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryTo" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Færsla til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="SecondaryCollectionReturnClaim">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Result of query on claims in secondary ceollection.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Niðurstaða fyrirspurnar um innheimtukröfur í milliinnheimtu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Key" type="clt:ClaimKey" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="PaymentsQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Query on payment.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Fyrirspurn um greiðslu.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Claimant" type="ct:PersonID">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The persion Id of the claimant.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Kennitala kröfuhafa.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Identifier" type="clt:Identifier" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The claim identifier belonging to the claimant used to filter the query.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Auðkenni kröfuhafa sem notað er til síunar.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DisposalAccount" type="ct:AccountID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The account to filter by. A convenience when many identifiers are connected to the same disposal account, to query for all payments connected to that account.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Reikningur til að sía eftir. Auðveldar leit að öllum kröfum sem tengjast tilteknum reikningi þegar mörg auðkenni vísa á sama reikning.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionDateFrom" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date from inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags frá og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TransactionDateTo" type="xs:date">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryFrom" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry from inclusive will be returned, starting from 1.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skilar færslum frá og með. Fyrsta færsla hefur númer 1</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryTo" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Færsla til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="DateSpanReferenceDate">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Reference dates, used in queries.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Tilgreinir viðmiðunardagsetningar, notað í leitum.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="DueDate">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Reference date is due date.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Gjalddagi er viðmiðunardagsetning.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FinalDueDate">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Reference date is final due date.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Eindagi er viðmiðunardagsetning.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CancellationDate">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Reference date is cancellation date.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Niðurfellingardagur er viðmiðunardagsetning.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CreationDate">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Reference date is creation date.</xs:documentation>
          <xs:documentation xml:lang="is-IS">TODO: Kanna hvort þetta eigi að vera með. Skráningardagur er viðmiðunardagsetning.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ClaimsQueryDateSpan">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Datespan of claims query.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Tímabil fyrir fyrirspurn um innheimtukröfur.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DateFrom" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date from inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags frá og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DateTo" type="xs:date" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Date to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Dags til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="DateSpanReferenceDate" type="clt:DateSpanReferenceDate"/>
  </xs:complexType>
  <xs:complexType name="ClaimsQuery">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Query on claims.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Fyrirspurn um innheimtukröfur.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Claimant" type="ct:PersonID">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The person identifier of the claimant whose data is being queried. If empty, data will be returned for all person ids linked to the authenticated user.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Kennitala þess kröfuhafa sem verið er að fletta gögnum upp fyrir. Gögnum er skilað fyrir alla kröfuhafa tengda innskráðum notanda ef gildið er ekki sent inn.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Identifier" type="clt:Identifier" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">The unique identifier of the claimant used to filter the query. If empty, data will be returned for all identifiers.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Auðkenni kröfuhafa sem notað er til síunar. Gögnum er skilað fyrir öll auðkenni ef gildið er ekki sent inn.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Period" type="clt:ClaimsQueryDateSpan">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Time period used to filter result set.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Tímabil sem sía á eftir.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Payor" type="ct:PersonID" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Person Id of the payor.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Kennitala greiðanda.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Status" type="clt:ClaimStatus" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Status of the claims.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Staða innheimtukrafna.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryFrom" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry from inclusive will be returned, starting from 1.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skilar færslum frá og með. Fyrsta færsla hefur númer 1</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="EntryTo" type="xs:unsignedInt" minOccurs="0">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Entry to inclusive.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Færsla til og með.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="QueryPaymentsResult">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Result of payment query.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Niðurstaða úr fyrirspurn um greiðslur.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TotalCount" type="xs:unsignedInt">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Total count of entries defined by the query.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skilar heildarfjölda færslna sem fyrirspurnin nær til.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Payments" type="clt:Payment" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="QueryClaimsResult">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">Result of claims query.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Skil fyrirspurnar um innheimtukröfur.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="TotalCount" type="xs:unsignedInt">
        <xs:annotation>
          <xs:documentation xml:lang="en-US">Total count of entries defined by the query.</xs:documentation>
          <xs:documentation xml:lang="is-IS">Skilar heildarfjölda færslna sem fyrirspurnin nær til.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Claims" type="clt:ClaimInfo" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClaimCollection">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">List of claims.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Listi af innheimtukröfum.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="clt:Claim" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ClaimKeyCollection">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">List of claim keys.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Listi af lyklum innheimtukrafna.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element ref="clt:ClaimKey" minOccurs="0" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Claim" type="clt:Claim" nillable="true">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">A claim.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Innheimtukrafa.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="ClaimKey" type="clt:ClaimKey" nillable="true">
    <xs:annotation>
      <xs:documentation xml:lang="en-US">A unique key of claim.</xs:documentation>
      <xs:documentation xml:lang="is-IS">Einstakur lykill innheimtukröfu.</xs:documentation>
    </xs:annotation>
  </xs:element>

</xs:schema>
